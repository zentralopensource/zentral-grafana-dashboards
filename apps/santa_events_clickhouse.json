          "pluginVersion": "4.12.0",
          "queryType": "logs",
          "rawSql": "SELECT \ncreated_at as \"timestamp\",\nconcat(\n  'sn=', serial_number,\n  ' dcsn=', payload.decision.:String,\n  ' sid=', coalesce(payload.signing_id.:String, 'UNKNOWN'),\n  ' fp=', payload.file_path.:String,\n  ' fn=', payload.file_name.:String\n) as title,\ncase\n  when payload.decision.:String LIKE 'BLOCK%' then 'error'\n  when payload.decision.:String = 'ALLOW_UNKNOWN' then 'warning'\n  else 'info'\nend as level,\nserial_number as serial_number,\npayload.decision.:String as decision,\npayload.signing_id.:String as signing_id,\npayload.file_path.:String as file_path,\npayload.file_name.:String as file_name\nFROM \"zentral\".\"zentral_events\"\nWHERE\ntype = 'santa_event'\nAND ( timestamp >= $__fromTime AND timestamp <= $__toTime )\nORDER BY timestamp\nDESC LIMIT 1000",
